<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>View Chart</title>
    <link rel="stylesheet" href="css/chart.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Josefin+Sans:ital,wght@0,200;0,300;0,400;0,500;1,300&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">
    <!--Ionicons-->
    <script
      type="module"
      src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"
    ></script>
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-dark background">
      <div class="container-fluid">
        <a class="navbar-brand fontPadding" href="/">
          <img src="CV_Logo.png" height="70px" width="100px" alt="CV_Logo" />
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarTogglerDemo03"
          aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
  
        <div class="collapse navbar-collapse" id="navbarTogglerDemo03">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item fontPadding">
              <a style="font-size: 23px" class="font nav-link" aria-current="page" href="/">Home</a>
            </li>
            <li class="nav-item fontPadding">
              <a style="font-size: 23px" class="font nav-link" href="about">About</a>
            </li>
            <li class="nav-item fontPadding">
              <a style="font-size: 23px" class="font nav-link" href="contact" tabindex="-1"
                aria-disabled="false">Contact</a>
            </li>
          </ul>
          <a href="login" style="text-decoration: none;"><button class="button-72">
            <%=label%>
            </button></a>
        </div>
      </div>
    </nav>
    <!-- <nav class="search__div">
      <div class="">
        <form class="d-flex" role="search">
          <input class="search__bar" name="coin" type="search" placeholder=" Search" aria-label="Search" onchange="chart">
        </form>
      </div>
    </nav> -->

    <div class="container-table">
      <table class="table">
        <tr>
          <th>Rank</th>
          <th>Icon</th>
          <th>Coin</th>
          <th>Code</th>
          <th>Price</th>
          <th>Gain/Loss</th>
          <th>Mkt Cap</th>
        </tr>
        <% for(let i=0; i<gotCoin.length;i++){ %> 
        <tr>
          <td><%=gotCoin[i].market_cap_rank%></td>
          <td><img src="<%=gotCoin[i].image%>" class="coinicon" alt="<%=gotCoin[i].name%>"></td>
          <td><%=gotCoin[i].name%></td>
          <td><%=gotCoin[i].symbol.toUpperCase();%></td>
          <td><%=gotCoin[i].current_price%></td>

          
          <% if(gotCoin[i].price_change_percentage_24h < 0){ %>
            <td class="red"><%=gotCoin[i].price_change_percentage_24h.toFixed(2)%> %</td>
          <%}else{%>
            <td class="green"><%=gotCoin[i].price_change_percentage_24h.toFixed(2)%> %</td>
          <%}%>

          <td><%=gotCoin[i].market_cap%></td>
          
        </tr>

        <%}%>
      </table>
    </div>
    <%- include("footer") -%>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
